{
  "name": "trpc-nuxt",
  "description": "End-to-end typesafe APIs in Nuxt applications.",
  "type": "module",
  "version": "0.4.1",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm build --watch\" \"pnpm --filter playground dev\"",
    "dev:prepare": "pnpm build && nuxt prepare playground",
    "prepublishOnly": "pnpm build",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "@trpc/client": "^10.0.0-proxy-beta.21",
    "@trpc/server": "^10.0.0-proxy-beta.21"
  },
  "dependencies": {
    "h3": "^0.8.6",
    "nanoid": "^4.0.0",
    "ohash": "^0.1.5",
    "ufo": "^0.8.6"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@trpc/client": "10.0.0-rc.8",
    "@trpc/server": "10.0.0-rc.8",
    "bumpp": "^8.2.1",
    "concurrently": "^7.5.0",
    "eslint": "^8.25.0",
    "tsup": "6.4.0",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "extends": [
      "@nuxtjs/eslint-config-typescript"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "off"
      ],
      "vue/multi-word-component-names": "off",
      "vue/no-multiple-template-root": "off"
    }
  },
  "eslintIgnore": [
    "*.json",
    "node_modules",
    "*.md",
    "dist",
    ".output"
  ]
}
