{
  "name": "trpc-nuxt",
  "type": "module",
  "version": "0.4.0-beta.15",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "require": "./dist/client/index.cjs",
      "import": "./dist/client/index.mjs"
    },
    "./module": {
      "import": "./module.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "client.d.ts",
    "module.mjs"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm build --watch\" \"pnpm --filter playground dev\"",
    "dev:prepare": "pnpm build && nuxt prepare playground",
    "prepublishOnly": "pnpm build",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "@trpc/client": "^10.0.0-proxy-beta.21",
    "@trpc/server": "^10.0.0-proxy-beta.21"
  },
  "dependencies": {
    "nuxt": "^3.0.0-rc.13",
    "h3": "^0.8.6",
    "nanoid": "^4.0.0",
    "ohash": "^0.1.5",
    "ufo": "^0.8.6"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@trpc/client": "10.0.0-rc.7",
    "@trpc/server": "10.0.0-rc.7",
    "bumpp": "^8.2.1",
    "concurrently": "^7.5.0",
    "eslint": "^8.25.0",
    "tsup": "6.4.0",
    "typescript": "^4.7.4"
  }
}
