{
  "name": "trpc-nuxt",
  "type": "module",
  "version": "0.4.0-beta.14",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "require": "./dist/client/index.cjs",
      "import": "./dist/client/index.mjs"
    },
    "./module": {
      "import": "./module.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "client.d.ts",
    "module.mjs"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm build --watch\" \"pnpm --filter playground dev\"",
    "prepublishOnly": "pnpm build",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "@trpc/client": "^10.0.0-proxy-beta.21",
    "@trpc/server": "^10.0.0-proxy-beta.21",
    "nuxt": "^3.0.0-rc.12"
  },
  "dependencies": {
    "h3": "^0.8.6",
    "nanoid": "^4.0.0",
    "ohash": "^0.1.5",
    "ufo": "^0.8.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/ni": "^0.18.3",
    "@trpc/client": "10.0.0-rc.3",
    "@trpc/server": "10.0.0-rc.3",
    "bumpp": "^8.2.1",
    "concurrently": "^7.5.0",
    "eslint": "^8.25.0",
    "nuxt": "3.0.0-rc.12",
    "tsup": "6.3.0",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "no-console": "warn"
    }
  }
}
