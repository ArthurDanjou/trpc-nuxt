{
  "name": "trpc-nuxt",
  "type": "module",
  "version": "0.2.4",
  "packageManager": "pnpm@7.5.0",
  "license": "MIT",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./api": {
      "import": "./dist/runtime/api.mjs",
      "types": "./dist/runtime/api.d.ts"
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "nr build",
    "build": "nuxt-module-build",
    "play": "nr build && nuxi dev playground",
    "build:playground": "nuxi build playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepare": "nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "3.0.0-rc.5",
    "@trpc/client": "^9.27.0",
    "@trpc/server": "^9.27.0",
    "@vueuse/core": "^8.9.3",
    "@vueuse/nuxt": "^8.9.3",
    "dedent": "^0.7.0",
    "defu": "^6.0.0",
    "h3": "^0.7.10",
    "pathe": "^0.3.0",
    "ufo": "^0.8.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@antfu/ni": "^0.16.2",
    "@nuxt/module-builder": "latest",
    "@types/dedent": "^0.7.0",
    "bumpp": "^7.2.0",
    "eslint": "^8.14.0",
    "nuxt": "3.0.0-rc.4",
    "ohash": "^0.1.0",
    "pnpm": "^7.5.0",
    "trpc-nuxt": "workspace:*",
    "zod": "^3.16.0"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "no-console": "warn"
    }
  },
  "volta": {
    "node": "16.14.2"
  }
}
